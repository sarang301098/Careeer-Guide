  	<servlet-class>com.controller.Barchart</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MysqlConnection</servlet-name>
  	<url-pattern>/MysqlConnection</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CheckLogin</servlet-name>
  	<url-pattern>/CheckLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
